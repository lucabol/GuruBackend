{
  "disabled": false,
  "bindings": [
    {
      "authLevel": "function",
      "name": "req",
      "type": "httpTrigger",
      "direction": "in",
      "route": "portfolios/{collection}/{cik}"
    },
    {
      "name": "port",
      "type": "documentDB",
      "databaseName": "guru-portfolios",
      "collectionName": "{collection}",
      "id": "{cik}",
      "connection": "AzureDocumentDB",
      "direction": "in"
    },
    {
      "name": "res",
      "type": "http",
      "direction": "out"
    }
  ]
}